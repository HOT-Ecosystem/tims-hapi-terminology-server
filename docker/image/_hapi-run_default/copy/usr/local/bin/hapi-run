#!/usr/bin/env bash
set -x
set -e
set -u
set -o pipefail
set -o noclobber
shopt -s nullglob

rm -rf target
env | sort
COMMAND="java ${HCS_HAPIRUN_JAVA_OPTIONS} -Dspring.config.location=\"${HCS_HAPIRUN_CONFIG_LOCATIONS}\" -jar ${HCS_HAPIRUN_JAR} ${HCS_HAPIRUN_ARGS}"
sleep 15
eval "$COMMAND"
echo HAPI RAN!!!